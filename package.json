{
  "name": "starter-template-react",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "description": "Starter template for React",
  "author": {
    "name": "sankeyangshu",
    "email": "sankeyangshu@gmail.com",
    "url": "https://github.com/sankeyangshu"
  },
  "license": "MIT",
  "homepage": "https://github.com/sankeyangshu/starter-template-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sankeyangshu/starter-template-react.git"
  },
  "bugs": {
    "url": "https://github.com/sankeyangshu/starter-template-react/issues"
  },
  "keywords": [
    "react",
    "template",
    "typescript",
    "@tanstack/react-router",
    "@tanstack/react-query",
    "i18n-react",
    "zustand",
    "tailwindcss v4",
    "iconify"
  ],
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=10.5.0"
  },
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "build:test": "tsc && vite build --mode test",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "release": "bumpp",
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.9.2",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.149.3",
    "@tanstack/react-router-devtools": "^1.149.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "es-toolkit": "^1.43.0",
    "i18next": "^25.7.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.3",
    "react-top-loading-bar": "^3.0.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@date-fns/tz": "^1.4.1",
    "@eslint-react/eslint-plugin": "^2.6.0",
    "@iconify/react": "^6.0.2",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/eslint-plugin-router": "^1.141.0",
    "@tanstack/router-plugin": "^1.149.3",
    "@types/node": "^24.10.8",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "ansis": "^4.2.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "eslint-plugin-better-tailwindcss": "^4.0.0",
    "eslint-plugin-format": "^1.3.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2",
    "simple-git-hooks": "^2.13.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "unplugin-svg-component": "^0.12.4",
    "vite": "^7.3.1",
    "vite-plugin-restart": "^2.0.0",
    "vite-tsconfig-paths": "^6.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm typecheck && pnpm lint-staged",
    "commit-msg": "pnpm exec commitlint --edit $1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
