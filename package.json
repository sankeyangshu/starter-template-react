{
  "name": "starter-template-react",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "description": "Starter template for React",
  "author": {
    "name": "sankeyangshu",
    "email": "sankeyangshu@gmail.com",
    "url": "https://github.com/sankeyangshu"
  },
  "license": "MIT",
  "homepage": "https://github.com/sankeyangshu/starter-template-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sankeyangshu/starter-template-react.git"
  },
  "bugs": {
    "url": "https://github.com/sankeyangshu/starter-template-react/issues"
  },
  "keywords": [
    "react",
    "template",
    "typescript",
    "@tanstack/react-router",
    "@tanstack/react-query",
    "i18n-react",
    "zustand",
    "tailwindcss v4",
    "iconify"
  ],
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=10.5.0"
  },
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "build:test": "tsc && vite build --mode test",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "release": "bumpp",
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-devtools": "^0.8.1",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-query-devtools": "^5.91.0",
    "@tanstack/react-router": "^1.139.3",
    "@tanstack/react-router-devtools": "^1.139.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@date-fns/tz": "^1.4.1",
    "@eslint-react/eslint-plugin": "^2.3.7",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/eslint-plugin-router": "^1.139.0",
    "@tanstack/router-plugin": "^1.139.3",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "ansis": "^4.2.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.1",
    "eslint-plugin-better-tailwindcss": "^3.7.11",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2",
    "simple-git-hooks": "^2.13.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "unplugin-icons": "^22.5.0",
    "vite": "^7.2.4",
    "vite-plugin-restart": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm typecheck && pnpm lint-staged",
    "commit-msg": "pnpm exec commitlint --edit $1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
